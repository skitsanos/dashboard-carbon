{
  "compilerOptions": {
    "baseUrl": ",",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "umi": [
        "src/.umi/exports"
      ]
    },
    "typeRoots": [
      "./src/@types"
    ],
    "types": [
      "node"
    ]
  },
  "include": [
    "mock/**/*",
    "src/**/*",
    "config/**/*",
    ".umirc.ts",
    "typings.d.ts"
  ],
  "exclude": [
    "node_modules/**/*",
    "lib",
    "es",
    "dist",
    "typings",
    "**/__test__",
    "test",
    "docs",
    "tests"
  ]
}